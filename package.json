  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/local-webhook.git"
  },
  "homepage": "https://github.com/RickWong/local-webhook",
  "keywords": [
    "localhost",
    "webhooks",
    "webhook",
    "ngrok",
